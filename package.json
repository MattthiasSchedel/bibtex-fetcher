{
  "name": "bibtex-manager",
  "displayName": "BibTex Manager",
  "description": "",
  "version": "0.1.2",
  "icon": "res/Bibfetch_Logo.png",
  "publisher": "matthiasschedel",
  "repository": {
    "type": "git",
    "url": "https://github.com/MattthiasSchedel/bibtex-fetcher"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bibtex-manager.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "BibTex Manager",
      "properties": {
        "yourExtension.libraryFilePath": {
          "type": "string",
          "default": "library",
          "description": "The file path for the library.bib file"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1"
  }
}
